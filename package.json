  "contributors": [{
    "name": "sean",
    "email": "famersbs@gmail.com"
  }],

  "browserify": {
    "transform": [ [ "babelify" , { "presets" : [ "es2015", "react" ]} ] ]
  },

  "scripts": {
    "watch": "watchify app.js -o public/js/bundle.js -v",
    "browserify": "babel app.js --presets react | browserify app.js | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "start": "npm run watch & nodemon server.js --exec babel-node"
  },

  "keywords": [
    "sample"
  ],

  "license": "MIT",

  "dependencies": {
    
    "babel": "5.8.21",
    "babel-loader": "5.3.2",
    "babel-relay-plugin": "^0.4.1",
    "classnames": "^2.1.3",
    "express": "^4.13.1",
    "express-handlebars": "~1.1.0",
    "express-graphql": "^0.4.4",
    "graphql": "^0.4.7",
    "graphql-relay": "^0.3.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-relay": "^0.5.0",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.10.1"
  },

  "devDependencies": {
    "browserify": "~6.0.3",
    "babelify": "~7.2.0",
    "nodemon": "^1.2.1",
    "uglify-js": "~2.4.15",
    "watchify": "~2.0.0",
    "babel-preset-react":"*",
    "babel-preset-es2015":"*"
  },

  "preferGlobal": true,
  "analyze": true,
  "license": "MIT"
}
